{
  "name": "react-faceless",
  "version": "0.0.1",
  "description": "Behavior patterns for React with your UI components",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:BananaBobby/react-faceless.git",
  "author": "Dmitry Belyaev <blv.dmitry@gmail.com> (https://bananabobby.github.io)",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook -s dist/storybook/",
    "build": "rm -rf dist && webpack",
    "lint": "eslint --ext .ts,.tsx src/**/*",
    "release": "yarn build && mv dist/* . && yarn publish",
    "postpublish": "rm -rf components utilities"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "devDependencies": {
    "@storybook/react": "^5.0.11",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-apply": "^0.12.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
